journal = {Journal of cardiovascular magnetic resonance : official journal of the Society for Cardiovascular Magnetic Resonance},
keywords = {Animal,Animals,Biomechanical Phenomena,Cine,Disease Models,Inbred C57BL,Left,Left: diagnosis,Left: etiology,Left: physiopathology,Magnetic Resonance Imaging,Mechanical,Mice,Myocardial Contraction,Obesity,Obesity: complications,Observer Variation,Predictive Value of Tests,Reproducibility of Results,Stress,Torsion,Ventricular Dysfunction,Ventricular Function},
