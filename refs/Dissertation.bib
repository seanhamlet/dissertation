journal = {Magnetic resonance in medicine},
keywords = {Algorithms,Echo-Planar Imaging,Echo-Planar Imaging: methods,Humans,Image Enhancement,Image Enhancement: methods,Image Interpretation, Computer-Assisted,Image Interpretation, Computer-Assisted: methods,Magnetic Resonance Imaging, Cine,Magnetic Resonance Imaging, Cine: instrumentation,Magnetic Resonance Imaging, Cine: methods,Phantoms, Imaging,Reproducibility of Results,Sensitivity and Specificity,Ventricular Dysfunction, Left,Ventricular Dysfunction, Left: diagnosis},
month = {oct},
