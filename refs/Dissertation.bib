keywords = {Adolescent,Adult,Arteries,Arteries: physiology,Coronary Vessels,Coronary Vessels: physiology,Female,Humans,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Middle Aged,Respiratory Physiological Phenomena},
