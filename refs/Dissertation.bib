journal = {European Journal of Echocardiography},
keywords = {Aged,Aging/*physiology,Case-Control Studies,Chi-Square Distribution,Echocardiography/*methods,Electrocardiography,Exercise Test,Female,Heart Failure/*physiopathology/*ultrasonography,Humans,Male,Prospective Studies,Reproducibility of Results,Stroke Volume/physiology,Ventricular Dysfunction, Left/*physiopathology/*ul},
