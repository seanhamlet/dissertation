volume = {7},
year = {1997}
}
@article{Harte2016,
abstract = {Respiratory disease is the leading cause of death in the UK. Methods for assessing pulmonary function and chest wall movement are essential for accurate diagnosis, as well as monitoring response to treatment, operative procedures and rehabilitation. Despite this, there is a lack of low-cost devices for rapid assessment. Spirometry is used to measure air flow expired, but cannot infer or directly measure full chest wall motion. This paper presents the development of a low-cost chest wall motion assessment system. The prototype was developed using four Microsoft Kinect sensors to create a 3D time-varying representation of a patient's torso. An evaluation of the system in two phases is also presented. Initially, static volume of a resuscitation mannequin with that of a Nikon laser scanner is performed. This showed the system has slight underprediction of 0.441 {\%}. Next, a dynamic analysis through the comparison of results from the prototype and a spirometer in nine cystic fibrosis patients and thirteen healthy subjects was performed. This showed an agreement with correlation coefficients above 0.8656 in all participants. The system shows promise as a method for assessing respiratory disease in a cost-effective and timely manner. Further work must now be performed to develop the prototype and provide further evaluations.},
author = {Harte, James M and Golby, Christopher K and Acosta, Johanna and Nash, Edward F and Kiraci, Ercihan and Williams, Mark A and Arvanitis, Theodoros N and Naidu, Babu},
doi = {10.1007/s11517-015-1433-1},
file = {:D$\backslash$:/research/sean/Mendeley/Harte et al. - 2016 - Chest wall motion analysis in healthy volunteers and adults with cystic fibrosis using a novel Kinect-based motion.pdf:pdf},
issn = {1741-0444 (Electronic)},
journal = {Medical {\&} biological engineering {\&} computing},
keywords = {Chest wall,Medical device design,Respiratory system diagnostic technique,Thoracic surgery,Thoracic wall,chest wall,medical device,respiratory system diagnostic technique,thoracic surgery,thoracic wall},
pmid = {26872677},
publisher = {Springer Berlin Heidelberg},
title = {{Chest wall motion analysis in healthy volunteers and adults with cystic fibrosis using a novel Kinect-based motion tracking system.}},
year = {2016}
}
@article{Stanton2009,
abstract = {BACKGROUND: Although global left ventricular systolic function is an important determinant of mortality, standard measures such as ejection fraction (EF) and wall motion score index (WMSI) have important technical limitations. The aim of this study was to compare global longitudinal speckle strain (GLS), an automated technique for measurement of long-axis function, with EF and WMSI for the prediction of mortality.

METHODS AND RESULTS: Of 546 consecutive individuals undergoing echocardiography for assessment of resting left ventricular function, 91 died over a period of 5.2+/-1.5 years. In addition to Simpson biplane EF, WMSI was determined by 2 experienced readers and GLS was calculated from 3 standard apical views using 2D speckle tracking. The incremental value of EF, WMSI, and GLS to significant clinical variables was assessed in nested Cox models. Clinical factors associated with outcome (model chi(2)=20.2) were age (hazard ratio [HR], 1.46; P{\textless}0.01), diabetes (HR, 1.88; P=0.01), and hypertension (HR, 1.59; P{\textless}0.05). Although addition of EF (HR, 1.23; P=0.03) or WMSI (HR, 1.28; P{\textless}0.01) added to the predictive power of clinical variables, the addition of GLS (HR, 1.45; P{\textless}0.001) caused the greatest increment in model power (chi(2)=34.9, P{\textless}0.001). GLS also provided incremental value in subgroups with EF {\textgreater}35{\%} and those with and without wall motion abnormalities. A GLS {\textgreater}or=-12{\%} was found to be equivalent to an EF {\textless}or=35{\%} for the prediction of prognosis. Intraobserver and interobserver variations for EF and GLS were similar.

CONCLUSIONS: GLS is a superior predictor of outcome to either EF or WMSI and may become the optimal method for assessment of global left ventricular systolic function.},
author = {Stanton, Tony and Leano, Rodel and Marwick, Thomas H},
doi = {10.1161/CIRCIMAGING.109.862334},
file = {:D$\backslash$:/research/sean/Mendeley/Stanton, Leano, Marwick - 2009 - Prediction of all-cause mortality from global longitudinal speckle strain comparison with ejection frac.pdf:pdf},
